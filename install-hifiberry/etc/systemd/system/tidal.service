[Unit]
Description=TidalConnect
Wants=network-online.target
After=network.target sound.target

[Service]
Type=simple
ExecStartPre=/opt/hifiberry/bin/bootmsg "Starting Tidal Connect"
ExecStart=ls -l
ExecStartPost=sleep 1

StandardOutput=journal
Restart=always
RestartSec=5

TimeoutStopSec=10

[Install]
WantedBy=multi-user.target

